<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saulo.saveitnologin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/save_it"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DayStreakActivity" />
        <activity android:name=".SliceActivity" />
        <activity android:name=".TipMyselfActivity" />
        <activity android:name=".InfoActivity"></activity>
    </application>

</manifest>